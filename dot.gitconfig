[alias]
    co = checkout
    cob = checkout -b
    ci = commit
    st = status -sb
    br = branch
    hist = log --pretty=format:\"%h %ad %an | %s%d\" --graph --date=short
    type = cat-file -t
    dump = cat-file -p
[color]
    ui = auto
[core]
    excludesfile = ~/.gitignore
[http]
	cookiefile = ~/.gitcookies
[merge]
	conflictstyle = diff3
