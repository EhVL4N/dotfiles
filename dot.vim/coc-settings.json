{
    "languageserver": {
        "ciderlsp": {
            "command": "/google/bin/releases/editor-devtools/ciderlsp",
            "args": [
                "--tooltag=coc-nvim",
                "--noforward_sync_responses"
            ],
            "filetypes": [
                "borg",
                "c",
                "cpp",
                "go",
                "java",
                "proto",
                "python",
                "textproto"
            ],
            "rootPatterns": ["google3"],
            "requireRootPattern": true
        },
        "golang": {
            "command": "gopls",
            "rootPatterns": ["go.mod", ".git/", ".hg/"],
            "filetypes": ["go"]
        }
    }
}
