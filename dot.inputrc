set bell-style none

# $if mode=vi
set keymap vi-insert
"jj": vi-movement-mode
"kk": vi-movement-mode
# $endif

# Insensitive case
set completion-ignore-case on

# disable twice key
set show-all-if-ambiguous on 

# show vi mode in prompt
set show-mode-in-prompt on
set vi-ins-mode-string +
set vi-cmd-mode-string :
