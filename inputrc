# Vimlike shortcuts
#"\ej":history-search-forward
#"\ek":history-search-backward
#"\eb":backward-word
#"\ew":forward-word
#"\eh":backward-char
#"\el":forward-char
#"\ex":delete-char
#"\ea":beginning-of-line
#"\ee":end-of-line
"\em":"make re\C-m"
"\es":"\C-asudo \C-m"

# Insensitive case
set completion-ignore-case on

# disable twice key
set show-all-if-ambiguous on 

# show vi mode in prompt
set show-mode-in-prompt on
